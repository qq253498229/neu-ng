{
  "applicationId": "js.neutralino.zero",
  "version": "1.0.0",
  "defaultMode": "window",
  "documentRoot": "/angular-src/dist/ng001/browser/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "nativeAllowList": [
    "app.*",
    "filesystem.readDirectory"
  ],
  "modes": {
    "window": {
      "title": "neu-react",
      "width": 800,
      "height": 500,
      "minWidth": 400,
      "minHeight": 200,
      "icon": "/angular-src/public/logo.png",
      "enableInspector": true,
      "injectGlobals": true
    }
  },
  "cli": {
    "binaryName": "neu-react",
    "extensionsPath": "/extensions/",
    "resourcesPath": "/angular-src/dist/ng001/browser/",
    "frontendLibrary": {
      "patchFile": "/angular-src/src/index.html",
      "devUrl": "http://localhost:4200",
      "projectPath": "/angular-src/",
      "initCommand": "npm install",
      "devCommand": "npm run start",
      "buildCommand": "npm run build",
      "waitTimeout": 20000
    },
    "binaryVersion": "5.5.0"
  }
}
